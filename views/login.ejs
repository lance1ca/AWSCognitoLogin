        <input class = "input-box" type="password" id="password" name="password" placeholder="Password">
        <br>
        <br>
        <a class = "input-link" href="/users/forgotPassword">Forgot password?</a>
        <br>
        <input class = "input-button" type="submit" value="Login">
        <br>
        <a class = "input-link" href="/users/register">Sign up</a>

        
       
    </form>
</div>


   

    

